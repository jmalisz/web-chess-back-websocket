{
  "name": "web-chess-back-websocket",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "build:server": "tsc",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:typecheck:watch": "tsc --noEmit --watch",
    "dev:server": "tsx src/app.ts --watch | pino-pretty",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --report-unused-disable-directives --max-warnings 0 --fix",
    "prettify": "prettier src --check",
    "prettify:fix": "prettier src --write",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "chess.js": "1.0.0-beta.6",
    "dotenv": "^16.3.1",
    "express": "~4.18.2",
    "helmet": "^7.0.0",
    "lodash-es": "^4.17.21",
    "pino": "^8.15.0",
    "socket.io": "4.6.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "20.4.7",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1",
    "husky": "^8.0.0"
  }
}
